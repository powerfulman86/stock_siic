<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
       <!-- Inherit Form View to Modify it -->
       <record id="stock_picking_form_date_modify" model="ir.ui.view">
           <field name="name">stock.picking form modify date done</field>
           <field name="model">stock.picking</field>
           <field name="inherit_id" ref="stock.view_picking_form"/>
           <field name="arch" type="xml">

               <xpath expr="//field[@name='date_done']" position="replace">
                   <field name="date_done" string="Effective Date"/>
               </xpath>

           </field>
       </record>


    </data>
</odoo>